{
    "entrypoints": [
        {
            "name": "subchain_verification",
            "framework": "plonky2",
            "baseDir": ".",
            "buildCommand": "cargo run --release --bin subchain_verification build && mv ./target/release/subchain_verification ./build/subchain_verification",
            "proveCommand": "env RUST_LOG=debug ./build/subchain_verification prove-wrapped --input-json input.json --wrapper-path /verifier-build"
        },
        {
            "name": "step",
            "framework": "plonky2",
            "baseDir": ".",
            "buildCommand": "cargo run --release --bin step build && mv ./target/release/step ./build/step",
            "proveCommand": "env RUST_LOG=debug ./build/step prove-wrapped --input-json input.json --wrapper-path /verifier-build"
        }
    ]
}
